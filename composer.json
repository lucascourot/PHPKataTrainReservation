{
  "name": "lucascourot/kata-train-reservation",
  "description": "Kata: reserve train seats",
  "type": "project",
  "require": {
    "php": "^7.1",
    "guzzlehttp/guzzle": "^6.3",
    "zendframework/zend-diactoros": "^1.6"
  },
  "require-dev": {
    "behat/behat": "^3.4",
    "friendsofphp/php-cs-fixer": "^2.8",
    "infection/infection": "^0.7.0",
    "mockery/mockery": "^1.0",
    "php-coveralls/php-coveralls": "^2.0",
    "phpunit/phpunit": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "TrainReservation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TrainReservationTest\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.1"
    },
    "preferred-install": {
      "*": "dist"
    },
    "bin-dir": "bin/",
    "sort-packages": true
  },
  "license": "MIT"
}
